<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ScheduleController">
   <children>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="500.0">
         <children>
            <Label text="Agenda" textFill="RED">
               <font>
                  <Font name="Arial Black" size="34.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#SetInput" text="Fichier Entrant" />
                  <Label fx:id="_input" />
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#SetOutput" text="Fichier Sortant" />
                  <Label fx:id="_output" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="10.0" prefWidth="400.0" spacing="20.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="20.0" />
               </padding>
               <children>
                  <Label text="Structure">
                     <font>
                        <Font name="Arial Black" size="15.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="_rOL" mnemonicParsing="false" text="OL">
                     <toggleGroup>
                        <ToggleGroup fx:id="type" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="_rBST" mnemonicParsing="false" text="BST" toggleGroup="$type" />
                  <RadioButton fx:id="_rEBST" mnemonicParsing="false" text="EBST" toggleGroup="$type" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="400.0" spacing="20.0">
               <padding>
                  <Insets right="20.0" />
               </padding>
               <children>
                  <Label text="Ordre">
                     <font>
                        <Font name="Arial Black" size="15.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="_rSPT" mnemonicParsing="false" style="-fx-rotate: 180;">
                     <toggleGroup>
                        <ToggleGroup fx:id="order" />
                     </toggleGroup>
                     <graphic>
                        <Label style="-fx-rotate: 180;" text="SPT" />
                     </graphic>
                  </RadioButton>
                  <RadioButton fx:id="_rLPT" mnemonicParsing="false" style="-fx-rotate: 180;" toggleGroup="$order">
                     <graphic>
                        <Label style="-fx-rotate: 180;" text="LPT" />
                     </graphic>
                  </RadioButton>
                  <RadioButton fx:id="_rWSPT" mnemonicParsing="false" style="-fx-rotate: 180;" toggleGroup="$order">
                     <graphic>
                        <Label style="-fx-rotate: 180;" text="WSPT" />
                     </graphic>
                  </RadioButton>
                  <RadioButton fx:id="_rFCFS" mnemonicParsing="false" style="-fx-rotate: 180;" toggleGroup="$order">
                     <graphic>
                        <Label style="-fx-rotate: 180;" text="FCFS" />
                     </graphic>
                  </RadioButton>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Label text="BackFilling">
               <font>
                  <Font name="Arial Black" size="15.0" />
               </font>
            </Label>
            <RadioButton mnemonicParsing="false" text="AVEC">
               <toggleGroup>
                  <ToggleGroup fx:id="backfilling" />
               </toggleGroup>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="SANS" toggleGroup="$backfilling" />
         </children>
      </HBox>
      <VBox alignment="CENTER">
         <children>
            <HBox alignment="CENTER" maxHeight="-Infinity" prefHeight="50.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label text="Durée Totale">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="_Makespan" alignment="CENTER" contentDisplay="CENTER" text="1" textAlignment="CENTER">
                           <font>
                              <Font name="Times New Roman" size="28.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Label text="Somme Pondéré de temps de fin">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="_WJCJ" alignment="CENTER" contentDisplay="CENTER" text="7" textAlignment="CENTER">
                           <font>
                              <Font name="Times New Roman" size="28.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Label text="Somme Pondéré de temps de réponse">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="_WJFJ" alignment="CENTER" contentDisplay="CENTER" text="3">
                           <font>
                              <Font name="Times New Roman" size="28.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Label text="Somme Pondéré de temps de retards">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="_WJTJ" alignment="CENTER" contentDisplay="CENTER" text="4">
                           <font>
                              <Font name="Times New Roman" size="28.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TableView fx:id="_tableView" prefHeight="800.0" prefWidth="800.0">
              <columns>
                <TableColumn editable="false" prefWidth="75.0" sortable="false" text="ID" />
                <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Temps" />
                  <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Date de Libération" />
                  <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Date de fin" />
                  <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Poids" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
</VBox>
